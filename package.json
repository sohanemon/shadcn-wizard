{
  "name": "shadcn-wizard",
  "displayName": "Shadcn Wizard",
  "description": "Nothing now",
  "publisher": "sohanemon",
  "version": "0.0.2",
  "repository": {
    "type": "git",
    "url": "https://github.com/sohanemon/vscode-extensions/tree/main/shadcn-anatomy"
  },
  "icon": "assets/icon.png",
  "engines": {
    "vscode": "^1.85.0"
  },
  "license": "MIT",
  "categories": [
    "Snippets",
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "snippets": [
      {
        "language": "javascriptreact",
        "path": "out/snippets.code-snippets"
      },
      {
        "language": "typescriptreact",
        "path": "out/snippets.code-snippets"
      }
    ],
    "commands": [
      {
        "command": "shadcn-wizard.installComponent",
        "title": "Shadcn: Install UI Component"
      },
      {
        "command": "shadcn-wizard.docs",
        "title": "Shadcn: Documentation"
      },
      {
        "command": "shadcn-wizard.initialize",
        "title": "Shadcn: Initialize UI"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "bun run compile && bun run build",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "bun run compile && bun run lint",
    "build": "bun out/bundler.js",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@types/bun": "^1.0.0"
  }
}
