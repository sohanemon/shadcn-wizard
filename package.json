{
  "name": "shadcn-wizard",
  "displayName": "Shadcn Wizard",
  "description": "Nothing now",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "shadcn-wizard.installComponent",
