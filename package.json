{
  "name": "shadcn-wizard",
  "displayName": "Shadcn/ui Wizard",
  "description": "A VS Code extension to streamline the use of Shadcn UI components.",
  "publisher": "sohanemon",
  "version": "0.0.7",
  "repository": {
    "type": "git",
    "url": "https://github.com/sohanemon/shadcn-wizard.git"
  },
  "icon": "assets/shad-wiz.png",
  "engines": {
    "vscode": "^1.85.0"
  },
  "license": "MIT",
  "categories": [
    "Snippets",
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "snippets": [
      {
        "language": "javascriptreact",
        "path": "out/snippets.code-snippets"
      },
      {
        "language": "typescriptreact",
        "path": "out/snippets.code-snippets"
      }
    ],
    "commands": [
      {
        "command": "shadcn-wizard.installComponent",
        "title": "Shadcn: Install UI Component"
      },
      {
        "command": "shadcn-wizard.docs",
        "title": "Shadcn: Documentation"
      },
      {
        "command": "shadcn-wizard.initialize",
        "title": "Shadcn: Initialize UI"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "pnpm run compile && pnpm run build",
    "vscode:publish": "vsce publish",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "build": "node out/bundler.js",
    "lint": "eslint src --ext ts",
    "test": "mocha -r ts-node/register src/test/**/*.ts"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.11.5",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8",
    "eslint": "^8.56.0",
    "mocha": "^10.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vsce": "^2.15.0"
  },
  "packageManager": "pnpm@10.11.0+sha512.6540583f41cc5f628eb3d9773ecee802f4f9ef9923cc45b69890fb47991d4b092964694ec3a4f738a420c918a333062c8b925d312f42e4f0c263eb603551f977"
}
